	"MONGODB_DB": "hippe",
	"AUTH_HOST": "localhost",
	"AUTH_PORT": 8003,
	"SEARCH_HOST": "localhost",
	"SEARCH_PORT": 9200,
	"LOG_LEVEL": 4,
	"ENABLE_MOCKUP": true,
	"DEFAULT_REPLY_EMAIL": "noreply@youremail.com",
